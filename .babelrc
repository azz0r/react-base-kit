{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "jsx-control-statements",
    "react-hot-loader/babel",
    "transform-async-to-generator",
    "syntax-async-functions",
    [
      "transform-runtime", {
        "polyfill": true,
        "regenerator": true
      }
    ]
  ]
}
